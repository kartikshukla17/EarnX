# U2U Solaris Mainnet Deployment Configuration
# Copy this file to .env and fill in your actual values

# Private key of the account that will deploy the contracts
# IMPORTANT: Never commit your actual private key to version control!
# Get your private key from MetaMask: Account Details > Export Private Key
PRIVATE_KEY=your_private_key_here

# Optional: U2U Explorer API key for contract verification
# You can get this from https://explorer.u2u.xyz (if they provide API keys)
U2U_EXPLORER_API_KEY=your_api_key_here

# Optional: Enable gas reporting (set to true to enable)
REPORT_GAS=false

# Network Configuration (already set in hardhat.config.js)
# RPC_URL=https://rpc-mainnet.u2u.xyz
# CHAIN_ID=39
# CURRENCY=U2U
# EXPLORER_URL=https://u2uscan.xyz/

# Example private key format (DO NOT USE THIS - IT'S JUST AN EXAMPLE):
# PRIVATE_KEY=0xc6b08c97b6039baeffaa806542acb680ae5e1ddda3e6c6496b8ac5c9aa1c256a
